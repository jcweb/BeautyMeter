<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="title"
            type="cn.yaman.view.NurseProcessTitle" />
    </data>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_48">

        <ImageView
            android:id="@+id/nurse_process_title_left"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_48"
            android:paddingStart="@dimen/dp_16"
            android:paddingEnd="@dimen/dp_08"
            android:onClick="finish"
            android:src="@mipmap/title_back_gold"
            android:layout_alignParentLeft="true"
            />
        <cn.yaman.view.PageIndicatorView
            android:id="@+id/nurse_process_title_indicator"
            android:layout_width="wrap_content"
            android:layout_centerInParent="true"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"/>
    </RelativeLayout>
</layout>
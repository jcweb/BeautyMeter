<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_02" />
            <stroke android:width="@dimen/dp_01" android:color="@color/colorPrimaryDark" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_02" />
            <stroke android:width="@dimen/dp_01" android:color="@color/colorPrimary" />
        </shape>
    </item>
</selector>